#logical operators
bool1=True
bool2=False
print('THE VALUE OF bool1 and bool2 is',bool1 and bool2)
print('THE VALUE OF bool1 or bool2 is',bool1 or bool2)
print('THE VALUE OF  not bool2 is', not bool2)
print('THE VALUE OF  not bool1 is', not bool1)
